<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox  xmlns="http://javafx.com/javafx/8.0.341" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.settings.SettingsController">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="1008.0">
         <children>
            <fx:include fx:id="showMachinePane" source="../showMachinePane/showMachinePane.fxml"  />
         </children></AnchorPane>
      <ScrollPane>
         <content>
            <HBox alignment="CENTER"    >
               <children>

                  <ScrollPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" >
                     <content>
                        <fx:include fx:id="editSettingsPane"  source="../editSettingsContainer/editSettingsPane.fxml" />
                     </content>
                  </ScrollPane>
                  <ScrollPane>
                     <content>
                        <fx:include fx:id="showSettingsPane" source="../showSettingsContainer/showSettingsPane.fxml" />
                     </content>
                  </ScrollPane>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </HBox>
         </content>
      </ScrollPane>
   </children>
</VBox>
