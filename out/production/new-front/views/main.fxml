<SplitPane dividerPositions="0.1631321370309951" orientation="VERTICAL" prefHeight="615.0" prefWidth="1026.0"
           xmlns="http://javafx.com/javafx/8.0.341" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="controllers.MainController">
